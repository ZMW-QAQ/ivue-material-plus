    "vite-plugin-dts": "^1.4.1",
    "vitest": "^0.28.4",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.24",
    "concurrently": "^7.6.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "react",
        "react-dom"
      ]
    }
  },
  "engines": {
    "node": ">= 16"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
