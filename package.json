{
  "name": "ivue-material-plus",
  "packageManager": "pnpm@7.3.0",
  "description": "A high quality UI components Library with Vue.js",
  "workspaces": [
    "packages/*",
    "examples"
  ],
  "scripts": {
    "cz": "czg",
    "dev": "pnpm -C examples dev",
    "build": "pnpm run -C build-install/build start",
    "build:styles": "pnpm run -C packages/styles build",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write --cache .",
    "prepare": "husky install",
    "typecheck": "run-p typecheck:web typecheck:examples typecheck:node typecheck:vite-config typecheck:vitest",
    "typecheck:web": "vue-tsc -p tsconfig.web.json --composite false --noEmit",
    "typecheck:node": "tsc -p tsconfig.node.json --noEmit",
    "typecheck:examples": "vue-tsc -p tsconfig.examples.json --composite false --noEmit",
    "typecheck:vite-config": "vue-tsc -p tsconfig.vite-config.json --composite false --noEmit",
    "typecheck:vitest": "vue-tsc -p tsconfig.vitest.json --composite false --noEmit",
    "test": "vitest",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "stub": "pnpm run -r --parallel stub",
    "postinstall": "pnpm stub",
    "release": "./publish/publish.sh"
  },
  "dependencies": {
    "@ivue-material-plus/components": "workspace:*",
    "@ivue-material-plus/directives": "workspace:*",
    "@ivue-material-plus/hooks": "workspace:*",
    "@ivue-material-plus/utils": "workspace:*",
    "@ivue-material-plus/test-utils": "workspace:*",
    "@ivue-material-plus/tokens": "workspace:*",
    "-": "0.0.1",
    "@antfu/utils": "^0.5.2",
    "@popperjs/core": "^2.11.6",
    "@types/lodash": "^4.14.186",
    "@types/lodash-es": "^4.17.6",
    "@types/lodash.throttle": "^4.1.7",
    "@vue/shared": "^3.2.37",
    "@vueuse/core": "^8.9.4",
    "async-validator": "^4.2.5",
    "countup.js": "^2.3.2",
    "fs": "^0.0.1-security",
    "hex-to-rgba": "^2.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "lodash.throttle": "^4.1.1",
    "material-icons": "^1.11.11",
    "mitt": "^3.0.0",
    "normalize-wheel-es": "^1.2.0",
    "path": "^0.12.7",
    "popper.js": "^1.16.1",
    "raf": "^3.4.1",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "peerDependencies": {
  },
  "devDependencies": {
    "@ivue-material-plus/build": "workspace:^0.0.1",
    "@ivue-material-plus/build-utils": "workspace:^0.0.1",
    "@ivue-material-plus/build-constants": "workspace:^0.0.1",
    "@ivue-material-plus/styles": "workspace:*",
    "@ivue-material-plus/eslint-config": "workspace:*",
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-modules-umd": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
